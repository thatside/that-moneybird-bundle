{
    "name":        "thatside/moneybird-bundle",
    "type":        "symfony-bundle",
    "description": "A Symfony2 bundle for working with Moneybird v2",
    "keywords":    ["moneybird", "symfony2"],
    "homepage":    "https://github.com/thatside/that-moneybird-bundle",
    "license":     "MIT",

    "authors": [
        {
            "name": "Bogdan Turchik",
            "email": "harammin@gmail.com"
        }
    ],

    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": ">=2.8",
        "picqer/moneybird-php-client": "~0.9",
        "beberlei/assert": "dev-master"
    },

    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "symfony/var-dumper": "^3.2",
        "phpstan/phpstan": "^0.8.5",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.0",
        "escapestudios/symfony2-coding-standard": "^3.0"
    },

    "autoload": {
        "psr-0": {
           "Thatside\\MoneybirdBundle": ""
        }
    },

    "target-dir": "Thatside/MoneybirdBundle"
}
